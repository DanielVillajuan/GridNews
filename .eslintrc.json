{
  "extends": ["./node_modules/standard/eslintrc.json", "next/core-web-vitals"],
  "plugins": ["plugin:testing-library/react", "plugin:jest-dom/recommended"],
  "overrides": [{
    "files":["*.ts", "*.tsx", "**/__tests__/**/*.[jt]s?(x)", "**/?(*.)+(spec|test).[jt]s?(x)"],
